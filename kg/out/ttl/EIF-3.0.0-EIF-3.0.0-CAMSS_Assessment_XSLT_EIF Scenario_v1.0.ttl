    cav:refersTo camssa:38af03c8-356d-4760-ae78-5396fd4ddb05 .

camssa:af02803a-5d8f-4d6a-9a04-90dc0cc9fe4c a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "XSLT is a business domain agnostic specification."@en ;
    cav:refersTo camssa:96cd4a59-49fc-4ee9-81ee-b5f8f7305f2f .

camssa:b34a6693-cfc4-475f-9492-7e360a8f7608 a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "The purpose of XSLT is not related to organisational interoperability. Therefore, this criterion is not applicable to the specification."@en ;
    cav:refersTo camssa:24043a82-8aa4-4cc4-9acb-36dbd4c02b9d .

camssa:b353f889-384c-44b0-a91e-0a3b427f1f59 a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "XSLT is extensively supported in the web browsers and it helps to transform a wide number of formats including HTML, XSL, Formatting Objects, PDF, PostScript and PNG."@en ;
    cav:refersTo camssa:848a2299-1726-4b9c-a902-2841bbc6efef .

