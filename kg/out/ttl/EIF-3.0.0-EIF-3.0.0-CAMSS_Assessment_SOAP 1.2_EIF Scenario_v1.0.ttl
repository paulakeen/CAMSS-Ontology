    cav:value "1"^^xsd:int .

camssa:ab1d68e0-865e-4551-b392-84e093f0bb33 a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "The purpose of SOAP 1.2 is not related to the long-term preservation of electronic records and other kinds of information. Therefore, this criterion is not applicable to the specification."@en ;
    cav:refersTo camssa:283ff705-4ada-4b98-9875-363b0e5c2ee5 .

