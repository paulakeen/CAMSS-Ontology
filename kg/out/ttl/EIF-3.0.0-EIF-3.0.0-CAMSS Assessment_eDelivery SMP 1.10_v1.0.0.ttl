    cav:value "1"^^xsd:int .

camssa:7958380a-b1c4-4f19-a72e-da498af7b1db a cav:Score,
        owl:NamedIndividual ;
    cav:assignedTo sc:c-19c639047dab46befb5839503733c8f1e7ed8278f788a089d11d2caee84a8f7c ;
    cav:value "2"^^xsd:int .

camssa:7ccfea4b-e5ff-41aa-98e9-150f0a79029d a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "The purpose of the specification is not related to e-accessibility. Therefore, e-Delivery SMP 1.10 is not easing the e-accessibility. "@en ;
    cav:refersTo camssa:4688bbe7-eaf5-4d23-8af1-2047bafc7266 .

camssa:7d4cc839-f5a5-4a91-a310-6af782baa1ba a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement """eDelivery SMP 1.10 is not included in any official supra-national catalogue. However, it is included in the full stack catalogue of eDelivery at CEF’s webpage. 
See: 
https://ec.europa.eu/cefdigital/wiki/display/CEFDIGITAL/Services+eDelivery """@en ;
    cav:refersTo camssa:065ce726-043d-47d4-9c50-3857cf6c55cf .

