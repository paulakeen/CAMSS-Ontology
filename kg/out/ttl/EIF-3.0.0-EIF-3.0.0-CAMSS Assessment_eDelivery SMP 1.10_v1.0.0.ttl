    cav:value "1"^^xsd:int .

camssa:d7f05392-eaab-4370-b5fd-6329a18807b5 a cav:Statement,
        owl:NamedIndividual ;
    cav:judgement "The purpose of the specification is not related to the publication of data as Open Data. Therefore, this criterion is not applicale to this specification."@en ;
    cav:refersTo camssa:2e84e4e4-dfd3-4ba9-9e0e-d2535f3e2234 .

